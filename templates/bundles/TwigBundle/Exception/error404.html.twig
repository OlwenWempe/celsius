{% extends 'base.html.twig' %}

{% block title %}Page non trouvée
{% endblock %}

{% block main %}
	<h1>Page non trouvée</h1>
	<p>La page que vous cherchez n'a pas été trouvée</p>
	<p id="status_code">Le code est :
		{{status_code}}</p>
	<p class="text-danger">Le texte est :
		{{status_text}}</p>
	<p>{{exception.message}}</p>
{% endblock %}
