{% extends 'base.html.twig' %}

{% block title %}TypeLieu index
{% endblock %}

{% block body %}
	<section class="section">
		<h1>TypeLieu index</h1>

		<table class="table">
			<thead>
				<tr>
					<th>Id</th>
					<th>Name</th>
					<th>actions</th>
				</tr>
			</thead>
			<tbody>
				{% for type_lieu in type_lieus %}
					<tr>
						<td>{{ type_lieu.id }}</td>
						<td>{{ type_lieu.name }}</td>
						<td>
							<a href="{{ path('app_type_lieu_show', {'id': type_lieu.id}) }}">show</a>
							<a href="{{ path('app_type_lieu_edit', {'id': type_lieu.id}) }}">edit</a>
						</td>
					</tr>
				{% else %}
					<tr>
						<td colspan="3">no records found</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>

		<a href="{{ path('app_type_lieu_new') }}">Create new</a>
	</section>
{% endblock %}
