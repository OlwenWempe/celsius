<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="./images/icon_celsius.png">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body class="container-fluid">
		{% include "_partials/_header.html.twig" %}
		<header>
			<nav class="navbar navbar-expand-lg bg-body-tertiary">
				<div class="container-fluid">
					<a class="navbar-brand" href="#"><img class="rounded-4" src='images/Logo_celsius.png' alt="Logo Celsius" height="70"></a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							<li class="nav-item">
								<a class="nav-link active" aria-current="page" href="#">Home</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="#">Link</a>
							</li>
							{% if is_granted("ROLE_ADMIN") %}
								<li class="nav-item me-auto mb-2 mb-lg-0">
									<a class="nav-link" href="{{ path('admin') }}">Page Admin</a>
								</li>
							{% endif %}
						</ul>

					</div>
				</div>
			</nav>
		</body>
	</body>
</html></header><main class="container">
{% for message in app.session.flashBag.get('info') %}
	<div class="w-50 alert alert-info" role="alert">
		{{ message }}
	</div>
{% endfor %}
{% for message in app.session.flashBag.get('error') %}
	<div class="w-50 alert alert-danger" role="alert">
		{{ message }}
	</div>
{% endfor %}
{% for message in app.session.flashBag.get('success') %}
	<div class="mx-auto w-50 alert alert-success" role="alert">
		{{ message }}
	</div>
{% endfor %}

{% block main %}{% endblock %}</main><footer></footer></body></html>
